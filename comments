
class FinInfoReader implements Closable {
    private static final String DATA_FILE_PATH = "data.file.path"

    Reader reader = new Reader("t.txt");

    String readNote() {
        return reader.nextLine();
    }

    public void close() throws IOException {
        reader.close();
    }


class TransactionDAO implements ...{
